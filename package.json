    "@react-native-community/cli-platform-ios": "^20.1.1",
    "@types/istanbul-reports": "^3.0.4",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "lightningcss": "^1.31.1",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^3.3.2",
    "typescript": "~5.9.2"
